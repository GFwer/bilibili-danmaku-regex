<item enabled="true">t=^.{1,5}助攻$</item>
<!-- 枯燥 -->
<item enabled="true">t=^枯燥$</item>
<!-- 害怕 -->
<item enabled="true">t=^害怕$</item>
<!-- 单字 -->
<item enabled="true">t=^[?？害危草淦]$</item>
<!-- 就这 -->
<item enabled="true">t=^/(就这)*\?*/$</item>
<!-- 打 call -->
<item enabled="true">t=^(\\.{1,6}\/\s{0,5})*$</item>
